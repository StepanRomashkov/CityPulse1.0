<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package = "com.jkmsteam.model.dto">
	<class name="Rating" table="ratings">
		<id name="id" column="ID" type="int">
			<generator class="increment"/>
		</id>
		<property type="long" column="user_id" name="userId"/>
		<property name="placeId" column="place_id" type="string"/>
		<property name="dead" column="dead" type="int"/>
		<property name="justRight" column="just_right" type="int"/>
		<property name="jumping" column="jumping" type="int"/>
		<property name="coverCharge" column="cover_charge" type="int"/>
		<property name="crowded" column="crowded" type="int"/>
		<property name="expensive" column="expensive" type="int"/>
		<property name="loud" column="loud" type="int"/>
		<property name="bigGroups" column="big_groups" type="int"/>
		<property name="smallGroups" column="small_groups" type="int"/>
		<property name="safePlace" column="safe_place" type="int"/>
		<property name="goodParking" column="good_parking" type="int"/>
	</class>
</hibernate-mapping>